@prefix : <http://www.semanticweb.org/angela/ontologies/2023/5/prosumer#> .
@prefix gn: <https://www.geonames.org/ontology#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix saref: <https://saref.etsi.org/core/> .
@prefix s4bldg: <https://saref.etsi.org/saref4bldg/> .
@prefix s4ener: <https://saref.etsi.org/saref4ener/> .
@prefix ic-data: <http://ontology.tno.nl/interconnect/datapoint#> .
@prefix ic-pwlm: <http://ontology.tno.nl/interconnect/powerlimit#> .
@prefix ic-tplg: <http://ontology.tno.nl/interconnect/topology#> .
@prefix prosumer: <http://www.semanticweb.org/angela/ontologies/2023/5/prosumer/> .
@prefix ic-device: <http://ontology.tno.nl/interconnect/device#> .
@prefix flexibility: <http://ontology.tno.nl/interconnect/flexibility#> .
@prefix kyrillos-es: <http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/> .
@base <http://www.semanticweb.org/angela/ontologies/2023/5/prosumer#> .

<http://www.semanticweb.org/angela/ontologies/2023/5/prosumer> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://ontology.tno.nl/interconnect/datapoint#belongsToTimeSeries
ic-data:belongsToTimeSeries rdf:type owl:ObjectProperty ;
                            owl:inverseOf ic-data:hasDataPoint ;
                            rdfs:domain ic-data:DataPoint ;
                            rdfs:range ic-data:TimeSeries ;
                            rdfs:comment "The relationship that connects a data point to a time series."@en ;
                            rdfs:label "belongs to time series"@en .


###  http://ontology.tno.nl/interconnect/datapoint#hasDataPoint
ic-data:hasDataPoint rdf:type owl:ObjectProperty ;
                     rdfs:domain ic-data:TimeSeries ;
                     rdfs:range ic-data:DataPoint ;
                     rdfs:comment "This relationship connects a time series to data point."@en ;
                     rdfs:label "has data point"@en .


###  http://ontology.tno.nl/interconnect/datapoint#hasTemporalResolution
ic-data:hasTemporalResolution rdf:type owl:ObjectProperty ;
                              rdfs:range time:TemporalDuration ;
                              rdfs:comment "The resolution is the distance between two measurement time-stapms. This only makes sense if the measurements are equidistant." ;
                              rdfs:label "has temporal resolution" .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer/composedBy
prosumer:composedBy rdf:type owl:ObjectProperty ;
                    rdfs:domain prosumer:Prosumer ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( prosumer:EnergyMeter
                                               prosumer:Generator
                                               prosumer:Load
                                               kyrillos-es:StorageSystem
                                             )
                               ] .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer/hasDirection
prosumer:hasDirection rdf:type owl:ObjectProperty ;
                      rdfs:domain kyrillos-es:StorageSystem ;
                      rdfs:range prosumer:EnergyDirection .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer/hasLocation
prosumer:hasLocation rdf:type owl:ObjectProperty ;
                     rdfs:domain kyrillos-es:StorageSystem ;
                     rdfs:range prosumer:EnergyStorageLocation .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer/hasPowerType
prosumer:hasPowerType rdf:type owl:ObjectProperty ;
                      rdfs:domain kyrillos-es:StorageSystem ;
                      rdfs:range prosumer:PowerType .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer/isConnected
prosumer:isConnected rdf:type owl:ObjectProperty ,
                              owl:SymmetricProperty .


###  http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/basedOnStateOfCharge
kyrillos-es:basedOnStateOfCharge rdf:type owl:ObjectProperty ;
                                 rdfs:domain kyrillos-es:EnergyEfficiency ;
                                 rdfs:range kyrillos-es:StateOfChargeMeasurement .


###  http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/isLocatedIn
kyrillos-es:isLocatedIn rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdfs:domain saref:Device ;
                        rdfs:range [ rdf:type owl:Class ;
                                     owl:unionOf ( ic-tplg:TopologicalLocation
                                                   geo:SpatialThing
                                                   s4bldg:BuildingSpace
                                                 )
                                   ] .


###  http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/isProtectedBy
kyrillos-es:isProtectedBy rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf owl:topObjectProperty ;
                          rdfs:domain saref:Device ;
                          rdfs:range ic-pwlm:NominalPowerLimit .


###  https://saref.etsi.org/isMeasuredByDevice
<https://saref.etsi.org/isMeasuredByDevice> rdf:type owl:ObjectProperty ;
                                            owl:inverseOf <https://saref.etsi.org/measuresProperty> ;
                                            rdfs:domain saref:Property ;
                                            rdfs:range saref:Device .


###  https://saref.etsi.org/isMeasuredIn
<https://saref.etsi.org/isMeasuredIn> rdf:type owl:ObjectProperty ;
                                      rdfs:range saref:UnitOfMeasure .


###  https://saref.etsi.org/measuresProperty
<https://saref.etsi.org/measuresProperty> rdf:type owl:ObjectProperty ;
                                          rdfs:domain saref:Device ;
                                          rdfs:range saref:Property .


###  https://saref.etsi.org/relatesToMeasurement
<https://saref.etsi.org/relatesToMeasurement> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf <https://saref.etsi.org/relatesToProperty> ;
                                              rdfs:domain saref:Property .


###  https://saref.etsi.org/relatesToProperty
<https://saref.etsi.org/relatesToProperty> rdf:type owl:ObjectProperty ;
                                           owl:inverseOf saref:relatesToMeasurement ;
                                           rdfs:range saref:Property .


###  https://saref.etsi.org/core/actsUpon
saref:actsUpon rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain saref:Command ;
               rdfs:range saref:State .


###  https://saref.etsi.org/core/consistsOf
saref:consistsOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain saref:Device ;
                 rdfs:range saref:Device .


###  https://saref.etsi.org/core/hasCommand
saref:hasCommand rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 owl:inverseOf saref:isCommandOf ;
                 rdfs:domain saref:Function ;
                 rdfs:range saref:Command .


###  https://saref.etsi.org/core/hasFunction
saref:hasFunction rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain saref:Device ;
                  rdfs:range saref:Function .


###  https://saref.etsi.org/core/hasState
saref:hasState rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain saref:Device ;
               rdfs:range saref:State .


###  https://saref.etsi.org/core/isCommandOf
saref:isCommandOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain saref:Command ;
                  rdfs:range saref:Function .


###  https://saref.etsi.org/core/isOfferedBy
saref:isOfferedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  owl:inverseOf saref:offers ;
                  rdfs:domain saref:Service ;
                  rdfs:range saref:Device .


###  https://saref.etsi.org/core/makesMeasurement
saref:makesMeasurement rdf:type owl:ObjectProperty ;
                       owl:inverseOf saref:measurementMadeBy ;
                       rdfs:domain saref:Device .


###  https://saref.etsi.org/core/measurementMadeBy
saref:measurementMadeBy rdf:type owl:ObjectProperty ;
                        rdfs:range saref:Device .


###  https://saref.etsi.org/core/offers
saref:offers rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain saref:Device ;
             rdfs:range saref:Service .


###  https://saref.etsi.org/core/relatesToMeasurement
saref:relatesToMeasurement rdf:type owl:ObjectProperty .


###  https://saref.etsi.org/core/represents
saref:represents rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain saref:Service ;
                 rdfs:range saref:Function .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer#hasEfficiency
:hasEfficiency rdf:type owl:DatatypeProperty ;
               rdfs:domain kyrillos-es:EnergyStorage ;
               rdfs:comment "Efficienza relativa al sistema di accumulo. Ovvero il rapporto tra l'energia rilasciata e l'energia immagazzinata (per raggiungere tale stato di carica)"@it .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer#hasMaxChargingPower
:hasMaxChargingPower rdf:type owl:DatatypeProperty ;
                     rdfs:domain kyrillos-es:EnergyStorage ;
                     rdfs:comment "Potenza massima (kW) relativa al sistema di accumulo in fase di carica, ovvero la massima quantità di potenza che la batteria può assorbire."@it .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer#hasMaxDischargingPower
:hasMaxDischargingPower rdf:type owl:DatatypeProperty ;
                        rdfs:domain kyrillos-es:EnergyStorage ;
                        rdfs:comment "Potenza massima (kW) relativa al sistema di accumulo in fase di scarica, anche nota come velocità di trasmissione di potenza."@it .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer#hasMinutesInterval
:hasMinutesInterval rdf:type owl:DatatypeProperty ;
                    rdfs:domain time:TemporalDuration .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer#hasValueEnergyAbsorbed
:hasValueEnergyAbsorbed rdf:type owl:DatatypeProperty ;
                        rdfs:range [ rdf:type rdfs:Datatype ;
                                     owl:onDatatype xsd:decimal ;
                                     owl:withRestrictions ( [ xsd:maxExclusive 0.0
                                                            ]
                                                          )
                                   ] .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer#hasValueEnergyMeter
:hasValueEnergyMeter rdf:type owl:DatatypeProperty ;
                     rdfs:domain :EnergyMeterMeasurement ;
                     rdfs:comment """Riferito al contatore (Energy Meter) in un'unità di tempo.
Se il valore è negativo si riferisce all'energia assorbita, se il valore è positivo si riferisce all'energia immessa."""@it .


###  http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/hasDurabilityInCycles
kyrillos-es:hasDurabilityInCycles rdf:type owl:DatatypeProperty ;
                                  rdfs:subPropertyOf owl:topDataProperty .


###  http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/hasEnergyGeneratedFromHydrogen
kyrillos-es:hasEnergyGeneratedFromHydrogen rdf:type owl:DatatypeProperty ;
                                           rdfs:domain kyrillos-es:HydrogenEnergyStorage ;
                                           rdfs:range [ rdf:type rdfs:Datatype ;
                                                        owl:onDatatype xsd:decimal ;
                                                        owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                                               ]
                                                                             )
                                                      ] .


###  http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/hasEnergyRequiredForHydrogen
kyrillos-es:hasEnergyRequiredForHydrogen rdf:type owl:DatatypeProperty ;
                                         rdfs:domain kyrillos-es:HydrogenEnergyStorage ;
                                         rdfs:range [ rdf:type rdfs:Datatype ;
                                                      owl:onDatatype xsd:decimal ;
                                                      owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                                             ]
                                                                           )
                                                    ] .


###  http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/hasHesEfficiency
kyrillos-es:hasHesEfficiency rdf:type owl:DatatypeProperty ;
                             rdfs:domain kyrillos-es:HydrogenEnergyStorage ;
                             rdfs:range [ rdf:type rdfs:Datatype ;
                                          owl:onDatatype xsd:decimal ;
                                          owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                                 ]
                                                               )
                                        ] .


###  http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/hasLosses
kyrillos-es:hasLosses rdf:type owl:DatatypeProperty ;
                      rdfs:domain kyrillos-es:EnergyStorage ;
                      rdfs:range [ rdf:type rdfs:Datatype ;
                                   owl:onDatatype xsd:decimal ;
                                   owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                          ]
                                                        )
                                 ] ;
                      rdfs:comment "Perdite relative al sistema di accumulo. Ovverto la quantità di SOC persa per unità di tempo a causa di perdite interne."@it .


###  http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/hasMaximumStateOfCharge
kyrillos-es:hasMaximumStateOfCharge rdf:type owl:DatatypeProperty ;
                                    rdfs:domain kyrillos-es:EnergyStorage ;
                                    rdfs:range [ rdf:type rdfs:Datatype ;
                                                 owl:onDatatype xsd:decimal ;
                                                 owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                                        ]
                                                                        [ xsd:maxInclusive 100
                                                                        ]
                                                                      )
                                               ] ;
                                    rdfs:comment "Stato di carica (%) relativo al sistema di accumulo, SOC massimo è il massimo stato di carica che la batteria può raggiongere in condizioni di sicurezza."@it .


###  http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/hasMinimumStateOfCharge
kyrillos-es:hasMinimumStateOfCharge rdf:type owl:DatatypeProperty ;
                                    rdfs:domain kyrillos-es:EnergyStorage ;
                                    rdfs:range [ rdf:type rdfs:Datatype ;
                                                 owl:onDatatype xsd:decimal ;
                                                 owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                                        ]
                                                                        [ xsd:maxInclusive 100
                                                                        ]
                                                                      )
                                               ] ;
                                    rdfs:comment "Stato di carica (%) relativo al sistema di accumulo, SOC minimo è il minimo stato di carica che la batteria può raggiongere in condizioni di sicurezza."@it .


###  http://www.semanticweb.org/cyrill/ontologies/2023/3/battery/hasStorageCapacity
kyrillos-es:hasStorageCapacity rdf:type owl:DatatypeProperty ;
                               rdfs:domain kyrillos-es:EnergyStorage ;
                               rdfs:range [ rdf:type rdfs:Datatype ;
                                            owl:onDatatype xsd:decimal ;
                                            owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                                   ]
                                                                 )
                                          ] ;
                               rdfs:comment "Capacità (kWh) relativa al sistema di accumulo, ovvero la massima energia disponibile nel sistema dopo una ricarica completa."@it .


###  https://saref.etsi.org/core/hasTimestamp
saref:hasTimestamp rdf:type owl:DatatypeProperty .


###  https://saref.etsi.org/core/hasValue
saref:hasValue rdf:type owl:DatatypeProperty ;
               rdfs:domain kyrillos-es:StateOfChargeMeasurement ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:onDatatype xsd:decimal ;
                            owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                   ]
                                                 )
                          ] .


#################################################################
#    Classes
#################################################################

###  http://ontology.tno.nl/interconnect/datapoint#DataPoint
ic-data:DataPoint rdf:type owl:Class ;
                  rdfs:subClassOf saref:Measurement .


###  http://ontology.tno.nl/interconnect/datapoint#TimeSeries
ic-data:TimeSeries rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty ic-data:hasDataPoint ;
                                     owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                     owl:onClass ic-data:DataPoint
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty ic-data:hasTemporalResolution ;
                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass time:TemporalDuration
                                   ] ;
                   rdfs:comment "An ordered sequence of data points of a quantity observed at spaced time intervals is referred to as a time series. Time series can be a result of prediction algorithm."@en ;
                   rdfs:label "Time series" .


###  http://ontology.tno.nl/interconnect/device#DeviceCluster
ic-device:DeviceCluster rdf:type owl:Class ;
                        rdfs:subClassOf saref:Device .


###  http://ontology.tno.nl/interconnect/device#Storage
ic-device:Storage rdf:type owl:Class ;
                  rdfs:subClassOf saref:Device .


###  http://ontology.tno.nl/interconnect/powerlimit#NominalPowerLimit
ic-pwlm:NominalPowerLimit rdf:type owl:Class ;
                          rdfs:subClassOf ic-pwlm:PowerLimit .


###  http://ontology.tno.nl/interconnect/powerlimit#PowerLimit
ic-pwlm:PowerLimit rdf:type owl:Class .


###  http://ontology.tno.nl/interconnect/topology#TopologicalLocation
ic-tplg:TopologicalLocation rdf:type owl:Class .


###  http://www.ontology-of-units-of-measure.org/resource/om-2/ActivePower
<http://www.ontology-of-units-of-measure.org/resource/om-2/ActivePower> rdf:type owl:Class ;
                                                                        rdfs:subClassOf <http://www.ontology-of-units-of-measure.org/resource/om-2/PowerUnit> .


###  http://www.ontology-of-units-of-measure.org/resource/om-2/PowerUnit
<http://www.ontology-of-units-of-measure.org/resource/om-2/PowerUnit> rdf:type owl:Class ;
                                                                      rdfs:subClassOf saref:UnitOfMeasure .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer#EnergyMeterMeasurement
:EnergyMeterMeasurement rdf:type owl:Class ;
                        rdfs:subClassOf ic-data:DataPoint ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty saref:measurementMadeBy ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass prosumer:EnergyMeter
                                        ] .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer#LoadMeasurement
:LoadMeasurement rdf:type owl:Class ;
                 rdfs:subClassOf ic-data:DataPoint ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty saref:measurementMadeBy ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass prosumer:Load
                                 ] .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer/AC
prosumer:AC rdf:type owl:Class ;
            rdfs:subClassOf prosumer:PowerType .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer/Bidirectional
prosumer:Bidirectional rdf:type owl:Class ;
                       rdfs:subClassOf prosumer:EnergyDirection ;
                       owl:disjointWith prosumer:Monodirectional .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer/Config01
prosumer:Config01 rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( prosumer:Prosumer
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty prosumer:composedBy ;
                                                               owl:someValuesFrom prosumer:Generator
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty prosumer:composedBy ;
                                                               owl:someValuesFrom prosumer:Load
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty prosumer:composedBy ;
                                                               owl:someValuesFrom prosumer:M1
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty prosumer:composedBy ;
                                                               owl:someValuesFrom prosumer:M2
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty prosumer:composedBy ;
                                                               owl:someValuesFrom [ owl:intersectionOf ( kyrillos-es:StorageSystem
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty prosumer:hasDirection ;
                                                                                                           owl:someValuesFrom prosumer:Monodirectional
                                                                                                         ]
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty prosumer:hasLocation ;
                                                                                                           owl:someValuesFrom prosumer:Production
                                                                                                         ]
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty prosumer:hasPowerType ;
                                                                                                           owl:someValuesFrom prosumer:DC
                                                                                                         ]
                                                                                                       ) ;
                                                                                    rdf:type owl:Class
                                                                                  ]
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf prosumer:Configuration ,
                                  [ owl:intersectionOf ( prosumer:Prosumer
                                                         [ rdf:type owl:Class ;
                                                           owl:complementOf [ rdf:type owl:Restriction ;
                                                                              owl:onProperty prosumer:composedBy ;
                                                                              owl:someValuesFrom prosumer:M3
                                                                            ]
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] .


###  http://www.semanticweb.org/angela/ontologies/2023/5/prosumer/Config02
prosumer:Config02 rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( prosumer:Prosumer
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty prosumer:composedBy ;
                                                               owl:someValuesFrom prosumer:Generator
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty prosumer:composedBy ;
                                                               owl:someValuesFrom prosumer:Load
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty prosumer:composedBy ;
                                                               owl:someValuesFrom prosumer:M1
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty prosumer:composedBy ;
                                                               owl:someValuesFrom prosumer:M2
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty prosumer:composedBy ;
                                                               owl:someValuesFrom [ owl:intersectionOf ( kyrillos-es:StorageSystem
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty prosumer:hasDirection ;
                                                                                                           owl:someValuesFrom prosumer:Bidirectional
                                                                                                         ]
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty prosumer:hasLocation ;
                                                                                                           owl:someValuesFrom prosumer:Production
                                                                                                         ]
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty prosumer:hasPowerType ;
                                                                                                           owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                                                owl:unionOf ( prosumer:AC
                                                                                                                                              prosumer:DC
                                                                                                                                            )
                                                                                                                              ]
                                                                                                         ]
                                                                                                       ) ;
                                                                                    rdf:type owl:Class
                                                                                  ]
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf prosumer:Configuration ,
                                  [ owl:intersectionOf ( prosumer:Prosumer
                                                         [ rdf:type owl:Class ;
                                                           owl:complementOf [ rdf:type owl:Restriction ;
                                                                              owl:onProperty prosumer:composedBy ;
                                                                              owl:someValuesFrom prosumer:M3
                                                                            ]
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] .


